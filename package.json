{
  "name": "fifa-play-zone",
  "version": "1.1.0",
  "description": "An Expo React Native app that consolidates FIFA+ Play Zone for the World Cup 2022 in Qatar.",
  "license": "MIT",
  "copyright": "Copyright (c) 2022 Justin Hartman",
  "author": "Justin Hartman <code@justhart.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinhartman/fifa-play-zone.git"
  },
  "bugs": {
    "url": "https://github.com/justinhartman/fifa-play-zone/issues"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "fix": "npx expo install --fix",
    "start": "npx expo start --tunnel",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "build": "npx eas build --no-wait",
    "preview": "npx eas build --no-wait --profile preview",
    "update:dev": "npx eas update --branch development",
    "update:stage": "npx eas update --branch staging",
    "update:prod": "npx eas update --branch production",
    "lint": "eslint -c .eslintrc.js *.jsx ./components/*.jsx ./constants/*.jsx ./styles/*.jsx --fix",
    "docs": "npx jsdoc App.jsx ./components/ ./constants/ ./styles/ -r -d ./docs"
  },
  "dependencies": {
    "@expo/cli": "^0.4.9",
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.4.1",
    "@react-navigation/native": "^6.0.14",
    "@react-navigation/stack": "^6.3.5",
    "eas-cli": "^2.7.1",
    "expo": "~47.0.6",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-webview": "11.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/ngrok": "^4.1.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-changelog": "^2.0.0",
    "grunt": "^1.5.3",
    "grunt-bump": "^0.8.0",
    "grunt-git": "^1.1.1"
  },
  "private": true
}
